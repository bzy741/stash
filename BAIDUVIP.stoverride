name: baiduvip
desc: 解锁百度倍速播放

http:
  mitm:
    - 'pan.baidu.com'
    
  script:
    - match: ^https:\/\/pan\.baidu\.com\/rest\/\d\.\d\/membership\/user
      name: BaiduCloud
      type: response
      require-body: true
      timeout: 10
script-providers:
  BaiduCloud:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/BaiduCloud.js
    interval: 86400
